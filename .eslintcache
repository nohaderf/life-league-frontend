Path":"145","messages":"146","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"147","messages":"148","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},{"filePath":"149","messages":"150","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"151","usedDeprecatedRules":"107"},{"filePath":"152","messages":"153","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"154","messages":"155","errorCount":0,"warningCount":3,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},{"filePath":"156","messages":"157","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"158","messages":"159","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/index.js",[],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/MainContainer.js",[],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/friends/Friends.js",["160"],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/league/tasks/AddTask.js",[],["161","162"],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/league/AddPlayers.js",[],["163","164"],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/league/Breakdown.js",["165"],"import React from 'react';\nimport BreakdownStats from './BreakdownStats';\n\nfunction Breakdown({ league }){\n\n    // have an array of userObjs from league.users\n    let leaguePlayers = league.users\n\n    // want to randomly select a userObj without repeating from array\n    // function randomMatchUp(){\n\n    const playerBreakdown = league.users.map(user => {\n        return <BreakdownStats key={user.id} user={user} />\n    })\n\n\n    return (\n        <>\n        <div className=\"league-header-div\">\n            <hr></hr>\n            <h1>Breakdown</h1>\n            <hr></hr>\n        </div>\n        <div className=\"breakdown-div\">\n            <table className=\"breakdown-table\">\n                <tbody>\n                    <tr>\n                        <th className=\"breakdown-header\">Player</th>\n                        <th className=\"breakdown-header\">Goal</th>\n                        <th className=\"breakdown-header\">Category</th>\n                        <th className=\"breakdown-header\">Points Earned</th>\n                    </tr>\n                    {playerBreakdown}\n                </tbody>\n            </table>\n        </div>\n        </>\n\n    )\n}\n\nexport default Breakdown;","/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/league/PlayerStats.js",["166","167","168"],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/App.js",["169"],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/reducers/rootReducer.js",[],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/league/tasks/TaskItem.js",[],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/league/LeaguePage.js",["170"],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/league/LeaguesNav.js",["171"],"import React, { useState } from 'react';\nimport { Link } from 'react-router-dom';\nimport NewLeague from './NewLeague';\n\nfunction LeaguesNav({ leagues, addNewLeague }){\n    const [leagueForm, setLeagueForm] = useState(false)\n\n    function newLeagueForm(){\n        setLeagueForm(!leagueForm)\n    }\n\n\n    const leagueLi = leagues.map(league => {\n        return (\n            <li onClick={(e) => {\n                e.preventDefault()\n               window.location.replace(`#${league.id}`)\n                }\n            }>\n                    <p className=\"league-nav-element\">{league.name}</p>\n            </li>\n        )\n    })\n\n    return(\n        <>\n        <nav className=\"league-nav\">\n            <ul>\n                {leagueLi}\n            </ul>\n            <button onClick={newLeagueForm} className=\"new-league-btn\"><i className=\"fas fa-plus-circle\"></i> New League</button>\n        </nav>\n        { leagueForm ? <NewLeague addNewLeague={addNewLeague} exitForm={newLeagueForm} closeForm={setLeagueForm} /> : null }\n        </>\n    )\n}\n\nexport default LeaguesNav;","/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/league/LeagueCard.js",["172"],"import React, { useState, useEffect } from 'react';\nimport Standings from './Standings';\nimport Breakdown from './Breakdown';\nimport AddPlayers from './AddPlayers';\nimport StandingsChart from './StandingsChart';\n\nfunction LeagueCard({ friends, league, onDeleteClick }){\n    const [showStandings, setShowStandings] = useState(true)\n    const [showChart, setShowChart] = useState(false)\n    const [showAddPlayers, setShowAddPlayers] = useState(false)\n    const [hidePlayersBtn, setHidePlayersBtn] = useState(false)\n    const [nonPlayers, setNonPlayers] = useState([])\n    const [players, setPlayers] = useState([])\n\n    const friendIds = friends.map(friend => friend.id) \n    const playerNames = players.map(player => player.first_name)\n    const playerPoints = players.map(player => player.total_points)\n\n    useEffect(() => {\n        fetch(`${process.env.REACT_APP_API_BASE_URL}/leagues/${league.id}`)\n        .then(r => r.json())\n        .then(leagueData => {\n            findNonPlayers(leagueData)\n            renderPlayers(leagueData)\n        })\n    }, [league.id])\n\n    function findNonPlayers(leagueObj){\n        const leaguePlayerIds = leagueObj.UserLeagues.map( userLeague => userLeague.user_id)\n        const notPlayerIds = friendIds.filter(id => !leaguePlayerIds.includes(id))\n        const notPlayerObjArr = friends.filter(friend => notPlayerIds.includes(friend.id))\n        setNonPlayers(notPlayerObjArr)\n    }\n\n    function renderPlayers(leagueObj){\n        setPlayers(leagueObj.users)\n    }\n\n    const usersInLeague = players.map( user => user.first_name)\n    const playersList = usersInLeague.join(\", \")\n\n    function handleAddPlayers(){\n        setShowAddPlayers(!showAddPlayers)\n        setHidePlayersBtn(!hidePlayersBtn)\n    }\n\n    function handleHidePlayers(){\n        setShowAddPlayers(!showAddPlayers)\n        setHidePlayersBtn(!hidePlayersBtn)\n    }\n\n    function handleNewPlayer(userLeagueData){\n        const newPlayer = friends.find(friend => friend.id === userLeagueData.user_id) \n        setPlayers([...players, newPlayer])\n        const updateNonPlayer = nonPlayers.filter(player => player.id !== userLeagueData.user_id)\n        setNonPlayers(updateNonPlayer)\n    }\n\n    function handleDelete(){\n        fetch(`${process.env.REACT_APP_API_BASE_URL}leagues/${league.id}`, {\n            method: \"DELETE\"\n        })\n        onDeleteClick(league)\n    }\n\n    function handleShowBreakdown(){\n        setShowStandings(false)\n    }\n\n    function handleShowStandings(){\n        setShowStandings(true)\n    }\n\n    function handleShowChart(){\n        setShowChart(!showChart)\n    }\n\n    return (\n        <div id={league.id} className=\"each-league\">\n            <nav className=\"inner-league-nav\">\n                <ul>\n                    <li>\n                        <p onClick={handleShowChart} className=\"chart-icon\"><i className=\"far fa-chart-bar\"></i></p> \n                    </li>\n                    <li>\n                        <p onClick={handleShowStandings} className=\"standings\">Standings</p> \n                    </li>\n                    <li>\n                        <p onClick={handleShowBreakdown} className=\"breakdown\">Breakdown</p>   \n                    </li>\n                </ul>\n            </nav>\n             <h1>{league.name}</h1>\n             <p className=\"league-details\"><span>Wager:</span> {league.notes}</p>\n             <p className=\"league-details\"><span>Players in League:</span> {playersList}</p>\n             { hidePlayersBtn ? <button onClick={handleHidePlayers}>Hide Players</button> : <button onClick={handleAddPlayers}>Add Players</button> } \n             <button onClick={handleDelete}>Delete League</button>\n            { showAddPlayers ? <AddPlayers league={league} nonPlayers={nonPlayers} onAddPlayer={handleNewPlayer} /> : null }\n            { showStandings? <Standings league={league} players={players} /> : <Breakdown league={league} players={players}/> }\n            { showChart ? <StandingsChart playerNames={playerNames} playerPoints={playerPoints} league={league} /> : null }\n            <hr className=\"bottom-hr\"></hr>\n        </div>\n    )\n}\n\nexport default LeagueCard;","/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/league/Standings.js",[],["173","174"],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/league/LeagueDetails.js",[],["175","176"],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/league/tasks/TaskRoster.js",[],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/Footer.js",["177"],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/About.js",["178"],"import React from \"react\"\nimport { Link } from \"react-router-dom\"\nimport ScrollToTop from \"./ScrollToTop\"\nimport logo from '../images/ll_black_logo.png'\n\nimport sketch from '../images/sketch.jpeg';\nimport violin from '../images/violin.jpeg';\nimport build from '../images/build.jpg';\nimport produce from '../images/produce.jpeg';\nimport leap from '../images/leap.jpeg';\nimport computer from '../images/lifeLeague_home.jpeg';\nimport friends from '../images/life_league_home.jpeg';\nimport cycling from '../images/cycling.jpeg'\n\nfunction About(){\n    return (\n        <>\n        <div className= \"main-div\">\n\n            <div className=\"info\">   \n                <div className=\"home-div\">\n                    <div className=\"fade-in-1\">\n                        <img className=\"home-img-1\" src={computer} alt=\"Life League\"></img>\n                        <div className=\"over-image\">\n                            <div className=\"slogan\">\n                                It's time to regain control of your goals.\n                            </div>\n                        </div>\n                    </div>\n\n                    <div className=\"fade-in-2\">\n                        <img className=\"home-img-2\" src={sketch} alt=\"Life League\"></img>\n                        <div className=\"over-image2\">\n                            <div className=\"slogan\">\n                                We all have a competitive side...\n                            </div>\n                        </div>\n                    </div>\n\n                    <div className=\"fade-in-3\">\n                        <img className=\"home-img-3\" src={friends} alt=\"Life League\"></img>\n                        <div className=\"over-image3\">\n                            <div className=\"slogan\">\n                                Challenge your friends and hold each other accountable.\n                            </div>\n                            <Link exact to=\"/rules\">\n                                <button className=\"get-started-btn\">Rules</button>\n                            </Link>\n                            <Link exact to=\"/signup\">\n                                <button className=\"sign-up-btn\">Sign Up</button>\n                            </Link>\n                        </div>\n                    </div> \n\n\n                    <div className=\"fun-div\">\n                        <div className=\"fun-card\">\n                            <div className=\"image-div\"><img className=\"image-ex\" src={violin} alt=\"KRUNCH FUN\"/></div>\n                            <p className=\"fun-q\">Log the time you've dedicated towards your goals</p>\n                        </div> \n\n                        <div className=\"fun-card\">\n                            <div className=\"image-div\"><img className=\"image-ex\" src={produce} alt=\"KRUNCH FUN\"/></div>\n                            <p className=\"fun-q\">Earn points</p>\n                        </div> \n\n                        <div className=\"fun-card\">\n                            <div className=\"image-div\"><img className=\"image-ex\" src={leap} alt=\"KRUNCH FUN\"/></div>\n                            <p className=\"fun-q\">Compete against your friends</p>\n                        </div> \n\n                        <div className=\"fun-card\">\n                            <div className=\"image-div\"><img className=\"image-ex\" src={build} alt=\"KRUNCH FUN\"/></div>\n                            <p className=\"fun-q\">Player with the most points, win.</p>\n                        </div>\n                    </div>\n\n                    <div className=\"fade-in-4\">\n                        <img className=\"home-img-4\" src={cycling} alt=\"Life League\"></img>\n                        <div className=\"over-image4\">\n                            <div className=\"slogan\">\n                                Join today.\n                            </div>\n                            <Link exact to=\"/signup\">\n                                <button className=\"sign-up-btn\">Sign Up</button>\n                            </Link>\n                        </div>\n                    </div> \n\n                </div>\n            </div>\n\n\n        </div>\n        <ScrollToTop />\n        </>\n    )\n}\n\nexport default About;","/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/NavBar.js",["179","180","181"],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/Rules.js",["182"],"import React from \"react\"\nimport { Link } from \"react-router-dom\"\nimport ScrollToTop from \"./ScrollToTop\"\n\n\nfunction Rules(){\n    return (\n        <div className= \"main-div\">\n            <div className= \"rules-div\">\n                <h1>TL;DR</h1>\n                <div className=\"rules-basics\">    \n                    <p>It's a game of integrity.</p>\n                    <p>Earn <strong>one</strong> point for every logged <strong>30-minute </strong> task dedication.</p>\n                    <p>The player with the most points at the end of the league, wins.</p>\n\n\n\n                </div>\n                <h1>THE BASICS</h1>\n                <div className=\"rules-basics\">    \n                    <ol>\n                        <li>The player who creates the league is the commissioner. The commissioner oversees the league.</li>\n                        <li>There needs to be at least two players. The more, the merrier.</li>\n                        <li>Players of the league (or the commissioner) will decide on the time frame of the league. The time frame can be chosen between four, six, or eight weeks.</li>\n                        <li>One point will be given for every 30 minutes a player logs in their Goal progress. The points earned will be counted towards an overall total. Points will only increment by one. So, if you dedicated 45 mins, you will still only be rewarded 1 point.</li>\n                        {/* <li>A player must log their progress on the day-of in order to earn points for the day. Players forfeit the chance to log daily results, and to claim points, if not logged by 11:59PM.</li> */}\n                        <li>Logging points is primarily based on a honor system. A player’s claims must be attested if other players of the league, or the commissioner, request proof or evidence.</li>\n                        <li>The higher the points, the higher the rank. The player with the most points by the end of the league, wins.</li>\n                    </ol>                       \n                </div>\n\n                {/* <h1>GOALS</h1>\n                <div className=\"rules-tasks\">    \n                    <ol>\n                        <li>All goals and progress in a player's roster will be counted in any league the player is a part of.</li>\n                        <li>New goals can be added at any time.</li>\n                        <li>A player can start logging and earning points under a goal as soon as it is created.</li>\n                        <li>Points earned from a deleted goal mid-league will <em>not</em> be removed from the player's total.</li> */}\n                        {/* <li>Tasks are actions to be done in order to work towards a player's goal.</li>\n                        <li>Tasks are decided and set upon the start of each league. They cannot be changed after the league starts.</li>\n                        <li>More than one task can be added to a player’s roster</li>\n                        <li>Players of the league (or the commissioner) will decide on Goal Categories. See more about Goal Categories down below.</li>\n                        <li>Each player can decide on at beginning of the week (week-to-week basis) whether to bench a particular Task or to put a Task into their lineup. Hint: A player may want to change their lineup depending on who they’re going up against that week.</li>\n                        <li>All Tasks in a player’s weekly lineup must be given status updates to gain points. Benched Tasks stand idle and will not gain or deduct player points.</li> */}\n                    {/* </ol>                       \n                </div> */}\n            </div>\n            <ScrollToTop />\n        </div>\n    )\n}\n\nexport default Rules;",["183","184"],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/SignUp.js",[],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/Login.js",[],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/Header.js",[],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/league/BreakdownStats.js",[],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/league/PlayerCard.js",[],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/Profile.js",["185","186"],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/friends/FriendCard.js",["187"],"import React from 'react';\nimport { Link } from 'react-router-dom';\n\nfunction FriendCard({ friend, onDeleteFriend, loadProfile }){\n\n    const { id, first_name, username, image_url } = friend\n\n    function handleDeleteClick(){\n        fetch(`http://localhost:3000/users/${id}`, {\n            method: \"PATCH\",\n            headers: {\n                \"Content-Type\": \"application/json\"\n            },\n            body: JSON.stringify({friend:false})\n        })\n        .then(r => r.json())\n        .then(onDeleteFriend)\n    }\n\n    function loadProfileClick(){\n        loadProfile(id)\n    }\n\n    return (\n        <div>\n            <p className=\"friends\">\n                <div onClick={loadProfileClick}>\n                    <img className=\"profile-thumbnail\" src={image_url} alt=\"username\"/>\n                    <span> {username} ({first_name})</span>\n                    <button className=\"remove-friend\" onClick={handleDeleteClick}>remove friend</button> \n                </div>\n                \n               \n            </p>\n            \n        </div>\n    )\n}\n\nexport default FriendCard;","/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/friends/UsersProfile.js",["188"],"import React, { useState, useEffect } from 'react';\n\nfunction UsersProfile({ id }){\n    const [user, setUser] = useState()\n    // const [currentUser, setCurrentUser] = useState(0)\n    const [isLoaded, setIsLoaded] = useState(false)\n\n    useEffect(() => {\n      fetch(`${process.env.REACT_APP_API_BASE_URL}users/${id}`)\n      .then(r => r.json())\n      .then(user => {\n          setUser(user)\n          setIsLoaded(true)\n      })\n  }, [id])\n\n    if (!isLoaded) return <h1>Loading Profile...</h1>\n\n    const { username, first_name, last_name, total_points, image_url } = user\n\n    // if (total_points > 30)\n    return (\n        <div className=\"main-div\">\n            <div className=\"profile-div\">\n                <div className=\"shift-up\">\n                    <img className=\"profile-pic\" src={image_url}></img><br></br>\n                    <h1>{first_name}</h1>\n                    <h2>Name: {first_name} {last_name}</h2>\n                    <h2>Username: {username}</h2>  \n                    { total_points ? <h2>Total earned points: {total_points ? total_points : 0 }</h2>  : \n                        <div>\n                            <p>Total Earned Points: 0</p>\n                            <h1> Zero points?! Better get started!</h1>\n                        </div> \n                    }\n\n                    { total_points > 30 ? \n                        <div>\n                            <h2 className=\"achievement-unlocked\">Achievements:</h2>\n                            <h2 className=\"achievement-unlocked\"><i className=\"fas fa-award\"></i> GOAL TREKKER</h2>\n                        </div> : null \n                    }\n                    \n                    { total_points > 50 ? \n                        <div>\n                            <h2 className=\"achievement-unlocked\">Achievements:</h2>\n                            <h1><i className=\"fas fa-medal\"></i> TRULY SKILLFUL</h1>\n                        </div> : null\n                    }\n\n                    {  total_points > 100 ? \n                        <div>\n                            <h2 className=\"achievement-unlocked\">Achievements:</h2>\n                            <h1><i className=\"fas fa-trophy\"></i>TRUE MASTER</h1>\n                        </div> : null\n                    }       \n                    \n                </div>\n            </div>\n      </div>\n    )\n}\n\nexport default UsersProfile;","/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/league/NewLeague.js",["189","190"],"import React, { useState } from 'react';\nimport { useParams, useHistory } from 'react-router-dom';\n// import AddPlayers from './AddPlayers';\n\nfunction NewLeague( { addNewLeague, exitForm, closeForm }){\n    // const params = useParams()\n    // const history = useHistory()\n    const [name, setName] = useState(\"\")\n    const [notes, setNotes] = useState(\"\")\n    const [duration, setDuration] = useState()\n    const users = []\n\n    function handleSubmit(e){\n        e.preventDefault()\n        const formData = {\n            name,\n            duration,\n            notes,\n            users\n        }\n\n        fetch(`${process.env.REACT_APP_API_BASE_URL}leagues/`, {\n            method: \"POST\",\n            headers: {\n                \"Content-Type\":\"application/json\"\n            },\n            body: JSON.stringify(formData)\n        })\n        .then(r => r.json())\n        .then(addNewLeague)\n        closeForm()\n    }\n\n    return (\n        <div className=\"form-popup\">\n            <button onClick={exitForm} className=\"exit\">X</button>\n            <h1>Create A New League</h1>\n            <div className=\"new-league-div\">    \n              <form className=\"new-league-form\" onSubmit={handleSubmit}>\n                \n                <p><label>Name your league:</label>\n                <input \n                    type=\"text\"\n                    name=\"name\"\n                    placeholder=\"League\"\n                    value={name}\n                    onChange={e => setName(e.target.value)}\n                />\n                </p>\n                <p><label>League Duration:</label>\n                <select onChange={e => setDuration(parseInt(e.target.value))} required=\"required\">\n                    <option selected value=\"null\">Choose Duration</option>\n                    <option value=\"1\">1 Week</option>\n                    <option value=\"4\">4 Weeks</option>\n                    <option value=\"6\">6 Weeks</option>\n                    <option value=\"8\">8 Weeks</option>\n                </select></p>\n                <p><label>Wager:</label>\n                <textarea\n                    type=\"textarea\"\n                    name=\"notes\"\n                    placeholder=\"The stakes are high...\"\n                    value={notes}\n                    onChange={e => setNotes(e.target.value)}\n                /></p>\n                {/* <p><label>Add players:</label></p>\n                <div className=\"add-players\">\n                    <AddPlayers />\n                </div> */}\n                <button type=\"submit\">Submit</button>\n            </form>  \n            </div>\n            \n        </div>\n    )\n}\n\nexport default NewLeague;","/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/league/TaskStats.js",[],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/CategoryChart.js",["191"],"/Users/fredahon/Flatiron/code/module_5/life-league-frontend/src/components/league/StandingsChart.js",["192"],"import React, { useState, useEffect } from 'react';\nimport { Bar } from 'react-chartjs-2';\n\nfunction StandingsChart({ playerPoints, playerNames, league }){\n    const [chartData, setChartData] = useState({})\n\n    function chart(){\n        setChartData( \n            {\n                labels: playerNames,\n                datasets: [\n                    {\n                        label: 'Points',\n                        data:  playerPoints,\n                        backgroundColor: [\"#ff9936\", \"#9e36ff\", \"#2085d8\", \"#acff52\" ,\"#fff000\", \"#fc00ff\", ],\n                        borderCapStyle: \"butt\",\n                        borderDash: [],\n                     